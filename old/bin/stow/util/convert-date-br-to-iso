#!/usr/bin/env python3

import sys
import datetime

FORMAT_IN = "%d/%m/%Y %H:%M"
FORMAT_OUT = "%Y-%m-%d %H:%M:%S"

# TODO multiple input formats?

for line in sys.stdin:
    txt = line.strip()
    # TODO error handling
    tm = datetime.datetime.strptime(txt, FORMAT_IN)
    print(tm.strftime(FORMAT_OUT))
